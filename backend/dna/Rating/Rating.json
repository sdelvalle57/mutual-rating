{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "rater",
    "value",
    "category"
  ],
  "properties": {
    "ratee": {
      "$id": "#/properties/rater",
      "type": "string",
      "title": "The Ratee Schema",
      "default": "",
      "examples": [
        "Hash2"
      ],
      "pattern": "^(.*)$"
    },
    "value": {
      "$id": "#/properties/value",
      "type": "integer",
      "title": "The Value Schema",
      "default": 0,
      "examples": [
        5
      ]
    },
    "category": {
      "$id": "#/properties/category",
      "type": "string",
      "title": "The Category Schema",
      "default": "",
      "examples": [
        "general"
      ],
      "pattern": "^(.*)$"
    }
  }
}
